{
  "name": "Web Task",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Frank Zhang",
  "browser_action": {
    "default_popup": "dist/index.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}